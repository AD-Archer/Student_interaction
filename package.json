{
  "name": "lp-form",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A student interaction form for launchpad philly created by Antonio Archer.",
  "keywords": [
    "nextjs",
    "prisma",
    "tailwindcss",
    "landing-page",
    "form"
  ],
  "author": "Antonio Archer <antonioarcher.dev@gmail.com>",
  "scripts": {
